{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch HTML file",
      "file": "${workspaceFolder}/${input:htmlFile}"
    }
  ],
  "inputs": [
    {
      "type": "pickString",
      "id": "htmlFile",
      "description": "Select the HTML file to launch",
      "options": [
        "index.html",
        "login.html",
        "register.html",
        "admin-dashboard.html",
        "studentmanagementboard.html",
        "dashboard.html",
        "forgot.html"
        
      ]
    }
  ]
}